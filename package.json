{
  "name": "how-front-end-works",
  "version": "1.0.0",
  "description": "How Front-End Works",
  "author": "De Voorhoede (https://www.voorhoede.nl)",
  "private": true,
  "scripts": {
    "prebuild": "touch ./.env && rimraf dist/ && npm run data",
    "build": "NODE_ENV=production run-s build:*",
    "build:app": "nuxt generate",
    "predata": "rimraf src/static/data/",
    "data": "dato dump",
    "predev": "rimraf dist/ && npm run data",
    "dev": "run-p dev:*",
    "dev:app": "nuxt",
    "lint": "eslint --ext .js,.vue --ignore-path .gitignore .",
    "precommit": "npm run lint",
    "proxy": "ngrok http 3000"
  },
  "dependencies": {
    "@nuxtjs/google-analytics": "2.2.0",
    "@nuxtjs/pwa": "3.0.0-beta.14",
    "@nuxtjs/sitemap": "0.1.1",
    "axios": "^0.19.0",
    "datocms-client": "0.6.2",
    "dotenv": "5.0.1",
    "lodash": "^4.17.15",
    "normalize.css": "8.0.0",
    "nuxt": "2.6.2",
    "vue-markdown": "^2.2.4"
  },
  "devDependencies": {
    "ajv": "6.5.0",
    "babel-eslint": "8.2.1",
    "eslint": "4.18.2",
    "eslint-friendly-formatter": "3.0.0",
    "eslint-loader": "2.1.1",
    "eslint-plugin-vue": "4.0.0",
    "ngrok": "3.0.1",
    "npm-run-all": "4.1.2",
    "postcss-custom-properties": "7.0.0",
    "postcss-import": "11.1.0",
    "rimraf": "2.6.2",
